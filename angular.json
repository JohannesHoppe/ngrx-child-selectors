{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "ngrx-child-selectors": {
      "projectType": "library",
      "root": "projects/ngrx-child-selectors",
      "sourceRoot": "projects/ngrx-child-selectors/src",
      "prefix": "ngrx",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:ng-packagr",
          "options": {
            "tsConfig": "projects/ngrx-child-selectors/tsconfig.lib.json",
            "project": "projects/ngrx-child-selectors/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "projects/ngrx-child-selectors/tsconfig.lib.prod.json"
            }
          }
        }
      }
    }
  },
  "defaultProject": "ngrx-child-selectors"
}
